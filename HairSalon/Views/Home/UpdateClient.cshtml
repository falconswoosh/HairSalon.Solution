@{
  Layout = "_Layout";
}

<h1>Update Client ID# @Model["client"].GetId(): @Model["client"].GetName()</h1>
<div class="stylistDetails">
  <form id="updateStylist" action="/stylist/@Model["stylist"].GetId()/client/@Model["client"].GetId()/update" method="post">
    <label for="name">Update Name</label>
    <input type="text" name="name" id="name">
    <button class="button" type="submit" name="button">Submit</button>
  </form>
</div>
